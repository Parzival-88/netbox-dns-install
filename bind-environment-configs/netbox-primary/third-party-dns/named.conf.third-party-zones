// Third-Party Zone Definitions
// Location: /etc/third-party-dns/named.conf.third-party-zones
//
// Zones where netbox-primary acts as SECONDARY, pulling from third-party primaries.
// These zones are then cascaded to our secondary DNS servers.
// Zone transfers FROM third-party use IP-based authentication (no TSIG).
// Zone transfers TO our secondaries use their respective TSIG keys.

// ============================================
// HPDCOCP ZONES
// Primary: 9.12.41.249
// ============================================

zone "hpdcocp.pok.stglabs.ibm.com" {
        type secondary;
        file "/etc/zones/third-party-dns/hpdcocp.pok.stglabs.ibm.com.db";
        primaries { 9.12.41.249; };
        allow-notify { 9.12.41.249; };
        allow-transfer {
                key "ns-9-12-46-4";
                key "ns-9-12-46-5";
                key "ns-9-11-227-25";
                key "ns-9-11-227-26";
                key "ns-9-3-1-200";
                key "ns-9-5-175-8";
                key "ns-9-12-16-2";
                key "ns-9-12-18-2";
        };
        notify yes;
        also-notify {
                9.12.46.4;
                9.12.46.5;
                9.11.227.25;
                9.11.227.26;
                9.3.1.200;
                9.5.175.8;
                9.12.16.2;
                9.12.18.2;
        };
};

zone "primary.hpdcocp.pok.stglabs.ibm.com" {
        type secondary;
        file "/etc/zones/third-party-dns/primary.hpdcocp.pok.stglabs.ibm.com.db";
        primaries { 9.12.41.249; };
        allow-notify { 9.12.41.249; };
        allow-transfer {
                key "ns-9-12-46-4";
                key "ns-9-12-46-5";
                key "ns-9-11-227-25";
                key "ns-9-11-227-26";
                key "ns-9-3-1-200";
                key "ns-9-5-175-8";
                key "ns-9-12-16-2";
                key "ns-9-12-18-2";
        };
        notify yes;
        also-notify {
                9.12.46.4;
                9.12.46.5;
                9.11.227.25;
                9.11.227.26;
                9.3.1.200;
                9.5.175.8;
                9.12.16.2;
                9.12.18.2;
        };
};

