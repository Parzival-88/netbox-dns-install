$TTL 3600
@       IN      SOA     netbox.sgn.ibm.com. sgn-netbox.ibm.com. (
                        2024010101      ; serial
                        3600            ; refresh
                        600             ; retry
                        86400           ; expire
                        3600 )          ; minimum

@       IN      NS      netbox.sgn.ibm.com.

; Catalog zone schema version (required)
version IN      TXT     "2"

; ============================================
; REVERSE ZONE MEMBERS
; ============================================
; Format: <unique-id>.zones IN PTR <zone-name>.
; Zone files stored in /etc/zones/reverse/ with .zone extension
;
; Example - to add 10.168.192.in-addr.arpa:
; e5f6g7h8.zones        IN      PTR     10.168.192.in-addr.arpa.
;
; NetBox will manage entries below this line
